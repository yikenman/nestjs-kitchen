    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.9",
    "@types/supertest": "^6.0.2",
    "express-session": "^1.18.2",
    "fastify": "^5.5.0",
    "jest": "^30.0.5",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=20.13.0"
  },
  "keywords": [
    "auth",
    "authentication",
    "authorization",
    "authz",
